<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="wallet" package="com.vnbig.wallet.db"/>
    <entity name="WalletAccount" table="WalletAccount" sequential="false" cacheable="false">
        <string name="currency" unique="false"/>
        <string name="code" unique="false"/>
        <long name="userCode" unique="false" required="true"/>
        <integer name="tag" unique="false" required="true"/>
        <decimal name="balance" unique="false" required="true"/>
        <decimal name="freeze" unique="false" required="true"/>
        <boolean name="status" unique="false" required="true"/>
        <long name="ves" unique="false"/>
        <string name="finger" unique="false"/>
        <string name="exchange" unique="false"/>
        <string name="remark" unique="false"/>
        <long name="cd" unique="false" required="true"/>
        <string name="cu" unique="false"/>
        <long name="lmd" unique="false"/>
        <string name="lmu" unique="false"/>
    </entity>
</domain-models>
