<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">


    <!-- Nomenclature : * name : "interfaceName" + "fieldName" + ".select" -->

    <!--广告选择栏-->
    <selection name="ad-trade-type-select">
        <option value="0">购买</option>
        <option value="1">出售</option>
    </selection>

    <selection name="ad-price-fixed-select">
        <option value="0">不固定</option>
        <option value="1">固定</option>
    </selection>

    <selection name="ad-status-select">
        <option value="0">已下架</option>
        <option value="1">已上架</option>
    </selection>

    <selection name="ad-is-recom-select">
        <option value="0">否</option>
        <option value="1">是</option>
    </selection>


    <!--审核选择栏0 — 取消 1 — 已提交，待审核 2 — 审核中 3 — 已通过 4 — 驳回 -->
    <selection name="merauth-check-status-select">
        <option value="0">取消</option>
        <option value="1">已提交，待审核</option>
        <option value="2">审核中</option>
        <option value="3">通过</option>
        <option value="4">驳回</option>
    </selection>

    <!--申诉类型 1 — 买家未付款 2 — 卖家未释放代币 3 — 长时间无响应 4 — 恶意下单 5 — 其他 -->
    <selection name="appeal-type-select">
        <option value="1">买家未付款</option>
        <option value="2">卖家未释放代币</option>
        <option value="3">长时间无响应</option>
        <option value="4">恶意下单</option>
        <option value="5">其他</option>
    </selection>

    <!--申诉人身份 0:用户 1：商家 -->
    <selection name="appeal-identityType-select">
        <option value="0">用户</option>
        <option value="1">商家</option>
    </selection>

    <!--申诉状态 0:未处理 1：已处理 -->
    <selection name="appeal-status-select">
        <option value="0">未处理</option>
        <option value="1">已处理</option>
        <option value="2">已取消</option>
    </selection>

    <!--申诉处理结果 1 — 释放卖家冻结币(系统取消) 2 — 增加买家可用币(完成)-->
    <selection name="appeal-result-select">
        <option value="1">释放卖家冻结币(系统取消)</option>
        <option value="2">增加买家可用币(完成)</option>
    </selection>

    <!--认证类型  1-ky1  2-ky2  3-ky3-->
    <selection name="merchantauthenticate-type-select">
        <option value="1">1-ky1</option>
        <option value="2">2-ky2</option>
        <option value="3">3-ky3</option>
    </selection>

    <!--otc交易流水 方向类型： 1、买入 2、卖出 3、转入 4、转出 5、锁仓 6、手续费-->
    <selection name="capitaldetail-type-select">
        <option value="1">买入</option>
        <option value="2">卖出</option>
        <option value="3">转入</option>
        <option value="4">转出</option>
        <option value="5">锁仓</option>
        <option value="6">手续费</option>
    </selection>

    <!--otc交易明细： 1、广告买入 2、广告卖出 3、转入 4、转出 5、锁仓 6、下单买入 7、下单卖出 8、下单买入手续费 9、下单卖出手续费 10、广告买入手续费 11、广告卖出手续费-->
    <selection name="capitaldetail-detail-select">
        <option value="1">广告买入</option>
        <option value="2">广告卖出</option>
        <option value="3">转入</option>
        <option value="4">转出</option>
        <option value="5">锁仓</option>
        <option value="6">下单买入</option>
        <option value="7">下单卖出</option>
        <option value="8">下单买入手续费</option>
        <option value="9">下单卖出手续费</option>
        <option value="10">广告买入手续费</option>
        <option value="11">广告卖出手续费</option>
    </selection>


</object-views>