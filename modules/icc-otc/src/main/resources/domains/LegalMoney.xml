<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
    <module name="otc" package="com.vnbig.otc.db"/>

    <entity name="LegalMoney"   table="legal_money" sequential="false"  cacheable="true">
        <string name="code" required="true" unique="true"/>
        <boolean name="enabled" required="true" default="0"/>


        <string name="cu"/>
        <string name="lmu"/>
        <datetime name="cd"/>
        <datetime name="lmd"/>
    </entity>



    <!--
    `id` varchar(32) NOT NULL,
  `code` varchar(16) NOT NULL COMMENT '法定货币code',
  `enabled` bit(1) NOT NULL DEFAULT b'0' COMMENT '是否禁用',
  `cd` datetime DEFAULT NULL,
  `md` datetime DEFAULT NULL,
  `cu` varchar(32) DEFAULT '',
  `lmu` varchar(32) DEFAULT ''

    -->
</domain-models>