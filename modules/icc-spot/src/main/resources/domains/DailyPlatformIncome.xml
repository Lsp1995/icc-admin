               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="financial" package="com.vnbig.financial.db"/>
    <entity name="DailyPlatformIncome" table="DailyPlatformIncome" sequential="false" cacheable="false">
        <string name="currency" unique="false"/>
        <integer name="statDate" unique="false"/>
        <decimal name="asiaPacificFee" unique="false"/>
        <decimal name="globalFee" unique="false"/>
        <decimal name="otcFee" unique="false"/>
        <decimal name="leverFee" unique="false"/>
        <decimal name="borrowCoinInterest" unique="false"/>
        <decimal name="contractFee" unique="false"/>
        <decimal name="withdrawFee" unique="false"/>
    </entity>
</domain-models>
