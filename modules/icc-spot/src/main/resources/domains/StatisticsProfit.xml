<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="trading" package="com.vnbig.trading.db"/>
    <entity name="StatisticsProfit" table="StatisticsProfit" sequential="false" cacheable="false">
        <decimal name="abonusAmount" unique="false"/>
        <string name="currency" unique="false"/>
        <integer name="examineEnabled" unique="false"/>
        <decimal name="feeAmount" unique="false"/>
        <boolean name="profitEnbaled" unique="false" required="true"/>
        <decimal name="rate" unique="false"/>
        <datetime name="summaryDate" unique="false"/>
        <decimal name="tradeAmount" unique="false"/>
        <string name="userAccount" unique="false"/>
        <string name="userCode" unique="false"/>
        <boolean name="satisfyPositions" unique="false" required="true"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
        <integer name="ve" unique="false"/>
        <string name="val" unique="false"/>
    </entity>
</domain-models>
