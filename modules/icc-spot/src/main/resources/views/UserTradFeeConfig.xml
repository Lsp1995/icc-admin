<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="userTradFeeConfig-grid" title="UserTradFeeConfig" model="com.vnbig.financial.db.UserTradFeeConfig"
          canDelete="false">
        <field name="enabled"/>
        <field name="exchange"/>
        <field name="feeType"/>
        <field name="rate"/>
        <field name="userId"/>
        <field name="varietyCode"/>
        <field name="cd" widget="date-time"/>
    </grid>
    <form name="userTradFeeConfig-form" title="UserTradFeeConfig" model="com.vnbig.financial.db.UserTradFeeConfig"
          canDelete="false">
        <panel title="UserTradFeeConfig" itemSpan="4">
            <field name="cd" title="Cd" readonly="true" widget="date-time"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true" widget="date-time"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="enabled"/>
            <field name="exchange"/>
            <field name="feeType"/>
            <field name="rate"/>
            <field name="userId"/>
            <field name="varietyCode"/>
        </panel>

    </form>
</object-views>
