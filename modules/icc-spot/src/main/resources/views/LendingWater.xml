          canEdit="false" canDelete="false" canAttach="false" width="large">
        <panel title="LendingWater" itemSpan="4" readonly="true">
            <field name="capitalDetailId"/>
            <field name="applyCode"/>
            <field name="variety"/>
            <field name="currency"/>
            <field name="user"/>
            <field name="amount" title="LendingAmount"/>
            <field name="lendingTime"/>
            <field name="interestRate"/>
            <field name="lendingSubject"/>
            <field name="cd" title="Cd"/>
