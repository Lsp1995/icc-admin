<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="thirdRechargeThreshold-grid" title="ThirdRechargeThreshold" model="com.vnbig.financial.db.ThirdRechargeThreshold" editable="false" customSearch="false" canNew="false" canEdit="false" canSave="false" canDelete="false" edit-icon="false">
        <field name="thirdSource" title="ThirdSource"/>
        <field name="currency" title="Currency"/>
        <field name="sumCeil" title="SumCeil"/>
        <field name="transferred" title="Transferred"/>
        <field name="threshold" title="Threshold"/>
        <field name="remark" title="Remark"/>
        <button name="resetButton" icon="fa-lock" onClick="com.vnbig.financial.controller.ThirdRechargeController:resetThirdRecharge" title="resetButton"/>
    </grid>
    <form name="thirdRechargeThreshold-form" title="ThirdRechargeThreshold" model="com.vnbig.financial.db.ThirdRechargeThreshold" canDelete="false" canEdit="false" canNew="false" width="large" canAttach="false">
        <panel title="ThirdRechargeThreshold" itemSpan="4" readonly="true">
            <field name="id" hidden="false"/>
            <field name="thirdSource" title="ThirdSource"/>
            <field name="currency" title="Currency"/>
            <field name="sumCeil" title="SumCeil"/>
            <field name="transferred" title="Transferred"/>
            <field name="threshold" title="Threshold"/>
            <field name="remark" title="Remark"/>
        </panel>
    </form>
</object-views>
