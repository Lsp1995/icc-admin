<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name=" financialTransferDetail-grid" title="FinancialTransferDetail"
          model="com.vnbig.financial.db.FinancialTransferDetail" canDelete="false" canEdit="false" canNew="false" edit-icon="false">
        <field name="amount"/>
        <field name="minerFee"/>
        <field name="outTradeNo"/>
        <field name="receiveAddress"/>
        <field name="sendAddress"/>
        <field name="walletId"/>
        <field name="remark"/>
        <field name="status"/>
    </grid>
    <form name="financialTransferDetail-form" title="FinancialTransferDetail"
          model="com.vnbig.financial.db.FinancialTransferDetail" canDelete="false" canNew="false" canEdit="false" width="large">
        <panel title="FinancialTransferDetail" itemSpan="4">
            <field name="cd" title="Cd" readonly="true" widget="date-time"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true" widget="date-time"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="amount"/>
            <field name="financialTransferId"/>
            <field name="minerFee"/>
            <field name="outTradeNo"/>
            <field name="remark"/>
            <field name="status"/>
            <field name="receiveAddress"/>
            <field name="sendAddress"/>
            <field name="walletId"/>
        </panel>
    </form>

</object-views>
