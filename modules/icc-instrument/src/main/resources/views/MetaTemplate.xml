<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name=" metaTemplate-grid" title="MetaTemplate" model="com.vnbig.bourse.db.MetaTemplate" canDelete="false">
        <field name="type" title="Types" width="80px"/>
        <field name="name"/>
        <field name="code"/>
        <field name="val"/>
        <field name="criteria" width="80px"/>
        <field name="language" width="100px"/>
        <field name="rank" width="80px"/>
        <field name="cd" widget="date-time"/>
    </grid>
    <form name="metaTemplate-form" title="MetaTemplate" model="com.vnbig.bourse.db.MetaTemplate" canDelete="false"
        width="large" canAttach="false">
        <panel title="MetaTemplate" itemSpan="4">
            <field name="type" title="Types"/>
            <field name="name"/>
            <field name="code"/>
            <field name="criteria"/>
            <field name="language"/>
            <field name="rank"/>
            <field name="val" widget="Text" colSpan="12"/>
            <field name="cd" title="Cd" readonly="true" widget="date-time"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true" widget="date-time"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
        </panel>
    </form>

</object-views>
