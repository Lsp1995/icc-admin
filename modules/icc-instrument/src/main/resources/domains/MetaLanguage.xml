<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="bourse" package="com.vnbig.bourse.db"/>
    <entity name="MetaLanguage" table="MetaLanguage" sequential="false" cacheable="false">
        <string name="code" unique="false"/>
        <boolean name="enabled" unique="false" required="true" selection="com.vnbig.market.enabled.select"/>
        <string name="language" unique="false"/>
        <string name="message" unique="false"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
        <string name="val" unique="false"/>
    </entity>
</domain-models>
