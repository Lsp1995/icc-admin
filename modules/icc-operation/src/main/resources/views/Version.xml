<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="version-grid" title="Version" model="com.vnbig.operation.db.Version" canDelete="false" orderBy="-cd">
        <field name="code"/>
        <field name="resources"/>
        <field name="device"/>
        <field name="upgrade"/>
        <field name="description"/>
        <field name="cd"/>
    </grid>
    <form name="version-form" title="Version" model="com.vnbig.operation.db.Version" canDelete="false" width="large">
        <panel title="Version" itemSpan="4">
            <field name="cd" title="Cd" readonly="true"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="code"/>
            <field name="resources" widget="Image"/>
            <field name="device"/>
            <field name="upgrade"/>
            <field name="description"/>
        </panel>
    </form>

</object-views>
