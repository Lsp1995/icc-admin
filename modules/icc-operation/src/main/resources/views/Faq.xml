<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="faq-grid" title="Faq" model="com.vnbig.operation.db.Faq" canDelete="false" orderBy="-cd">
        <field name="language"/>
        <field name="question"/>
        <field name="rank"/>
        <field name="enabled"/>
        <field name="cd"/>
    </grid>
    <form name="faq-form" title="Faq" model="com.vnbig.operation.db.Faq" canDelete="false" width="large">
        <panel title="Faq" itemSpan="4">
            <field name="cd" title="Cd" readonly="true"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="language"/>
            <field name="categoryId"/>
            <field name="hot"/>
            <field name="question"/>
            <field name="tags"/>
            <field name="metaKeywords"/>
            <field name="metaDescription"/>
            <field name="digest"/>
            <field name="response" colSpan="12" widget="html"/>
        </panel>
    </form>

</object-views>
