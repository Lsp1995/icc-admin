<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="notice-grid" title="Notice" model="com.vnbig.operation.db.Notice" canDelete="false" orderBy="-cd">
        <field name="language"/>
        <field name="title"/>
        <field name="noticeAbstract"/>
        <field name="enabled"/>
        <field name="releaseTime"/>
        <field name="cd"/>
    </grid>
    <form name="notice-form" title="Notice" model="com.vnbig.operation.db.Notice" canDelete="false" width="large">
        <panel title="Notice" itemSpan="4">
            <field name="cd" title="Cd" readonly="true"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="title"/>
            <field name="author"/>
            <field name="language"/>
            <field name="releaseTime"/>
            <field name="noticeAbstract"/>
            <field name="enabled"/>
            <field name="rank"/>
            <field name="context" widget="html"/>
        </panel>
    </form>

</object-views>
