<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="award-grid" title="Award" model="com.vnbig.operation.db.Award" canDelete="false" canNew="false" canEdit="false" edit-icon="false">
        <field name="name"/>
        <field name="currency"/>
        <field name="amount"/>
        <field name="enabled"/>
    </grid>
    <form name="award-form" title="Award" model="com.vnbig.operation.db.Award" canDelete="false" canEdit="false" canNew="false" width="large">
        <panel title="Award" itemSpan="4" readonly="true">
            <field name="cd" title="Cd" readonly="true"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="currency"/>
            <field name="name"/>
            <field name="amount" />
            <field name="enabled"/>
        </panel>
    </form>

</object-views>
