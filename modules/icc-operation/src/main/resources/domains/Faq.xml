<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="operation" package="com.vnbig.operation.db"/>
    <entity name="Faq"  table="Faq" sequential="false" cacheable="false">
        <string name="categoryId" unique="false"/>
        <string name="digest" unique="false" large="true"/>
        <boolean name="enabled" required="true" unique="false"/>
        <integer name="hot" required="true" unique="false"/>
        <string name="language" unique="false" selection="com.vnbig.operation.language.select"/>
        <string name="metaDescription" unique="false" large="true"/>
        <string name="metaKeywords" unique="false" large="true"/>
        <string name="question" unique="false" large="true"/>
        <integer name="rank" required="true" unique="false"/>
        <integer name="resolved" required="true" unique="false"/>
        <string name="response" unique="false" required="true" large="true"/>
        <integer name="status" unique="false"/>
        <string name="tags" unique="false" large="true"/>
        <integer name="unresolved" required="true" unique="false"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
        <integer name="ve" unique="false"/>
        <string name="val" unique="false"/>
    </entity>
</domain-models>
