<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="operation" package="com.vnbig.operation.db"/>
    <entity name="Income"  table="Income" sequential="false" cacheable="false">
        <decimal name="amount" unique="false"/>
        <string name="currency" unique="false"/>
        <decimal name="fee" unique="false"/>
        <decimal name="incomeMoney" unique="false"/>
        <string name="orderId" unique="false"/>
        <long name="time" required="true" unique="false"/>
        <integer name="type" unique="false"/>
        <string name="userId" unique="false"/>
        <string name="incomeUserCode" unique="false"/>
        <string name="user" column="`user`" unique="false"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
        <integer name="ve" unique="false"/>
        <string name="val" unique="false"/>
    </entity>
</domain-models>
