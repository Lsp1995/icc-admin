<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="operation" package="com.vnbig.operation.db"/>
    <entity name="FeedBack"  table="FeedBack" sequential="false" cacheable="false">
        <string name="address" unique="false"/>
        <string name="categoryId" unique="false"/>
        <string name="content" unique="false" large="true"/>
        <string name="email" unique="false"/>
        <string name="mobile" unique="false"/>
        <string name="phone" unique="false"/>
        <string name="qq" unique="false"/>
        <string name="reply" unique="false" large="true"/>
        <boolean name="status" unique="false" required="true"/>
        <string name="wechat" unique="false"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
        <integer name="ve" unique="false"/>
        <string name="val" unique="false"/>
    </entity>
</domain-models>
