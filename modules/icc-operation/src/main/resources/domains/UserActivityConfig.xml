<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="operation" package="com.vnbig.operation.db"/>
    <entity name="UserActivityConfig"  table="UserActivityConfig" sequential="false" cacheable="false">
        <string name="configType" unique="false"/>
        <string name="configName" unique="false"/>
        <integer name="status" unique="false"/>
        <string name="tradeType" unique="false"/>
        <string name="awardAccount" unique="false"/>
        <string name="awardCurrency" unique="false"/>
        <decimal name="awardAmount" unique="false"/>
        <decimal name="feeReturnRate" unique="false"/>
        <datetime name="cd" unique="false"/>
        <datetime name="lmd" unique="false"/>
        <string name="cu" unique="false"/>
        <string name="lmu" unique="false"/>
    </entity>
</domain-models>
