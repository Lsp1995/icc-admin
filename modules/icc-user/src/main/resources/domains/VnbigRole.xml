<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

    <module name="user" package="com.vnbig.user.db"/>
    <entity name="VnbigRole"  table="Role" sequential="false" cacheable="false">
        <string name="code" unique="false"/>
        <string name="description" unique="false"/>
        <boolean name="enabled" unique="false" required="true"/>
        <string name="name" unique="false"/>
        <integer name="rank" unique="false" required="true"/>
    </entity>
</domain-models>
