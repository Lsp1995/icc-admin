<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

    <grid name="apiResource-grid" title="ApiResource" model="com.vnbig.user.db.Apiresource" canDelete="false" orderBy="-cd">
        <field name="modelCode"/>
        <field name="modelName"/>
        <field name="modelType"/>
        <field name="modelUrl"/>
        <field name="paramKey"/>
        <field name="paramValue"/>
        <field name="enabled"/>
        <field name="cd" widget="date-time"/>
    </grid>
    <form name="apiResource-form" title="ApiResource" model="com.vnbig.user.db.Apiresource" canDelete="false" width="large">
        <panel title="ApiResource" itemSpan="6">
            <field name="cd" title="Cd" readonly="true" widget="date-time"/>
            <field name="cu" title="Cu" readonly="true"/>
            <field name="lmd" title="Lmd" readonly="true" widget="date-time"/>
            <field name="lmu" title="Lmu" readonly="true"/>
            <field name="ve" title="Ve" readonly="true"/>
            <field name="val" title="Val" readonly="true"/>
            <field name="modelCode"/>
            <field name="modelName"/>
            <field name="modelType"/>
            <field name="modelUrl"/>
            <field name="paramKey"/>
            <field name="paramValue"/>
            <field name="enabled" title="Enabled"/>
            <field name="description"/>
        </panel>
    </form>

</object-views>
